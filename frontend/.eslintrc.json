{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/order": [
      "error",
      {
        "groups": [["builtin", "external", "internal", "parent", "sibling", "index"]],
        "newlines-between": "never"
      }
    ]
  }
}
